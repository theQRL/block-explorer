    "axios": "^0.27.2",
    "babel-runtime": "^6.26.0",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.0.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "elliptic": "^6.5.4",
    "es5-shim": "^4.6.7",
    "fs": "0.0.1-security",
    "ini": "^3.0.0",
    "jquery": "^3.6.0",
    "jquery-qrcode": "^1.0.0",
    "jscrypto": "^1.0.3",
