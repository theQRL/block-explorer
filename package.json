{
  "name": "block-explorer",
  "version": "1.5.1",
  "description": "QRL block explorer",
  "main": "index.js",
  "scripts": {
    "build:css": "tailwindcss -i ./public/tailwind.css -o ./public/tailwind-output.css",
    "build:css:watch": "tailwindcss -i ./public/tailwind.css -o ./public/tailwind-output.css --watch",
    "lint": "eslint --ext .js client imports server",
    "lint:fix": "eslint --ext .js --fix client imports server",
    "start": "meteor",
    "dev": "concurrently \"npm run build:css:watch\" \"meteor\""
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.6.13",
    "@theqrl/explorer-helpers": "^2.8.0",
    "@theqrl/nft-providers": "^1.1.0",
    "@theqrl/validate-qrl-address": "^3.0.1",
    "axios": "^0.27.2",
    "babel-runtime": "^6.26.0",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.3.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "elliptic": "^6.6.1",
    "es5-shim": "^4.6.7",
    "fs": "0.0.1-security",
    "ini": "^3.0.1",
    "jquery": "^3.7.1",
    "jquery-qrcode": "^1.0.0",
    "jscrypto": "^1.0.3",
    "json-formatter-js": "^2.5.23",
    "lodash": "^4.17.21",
    "lucide": "^0.545.0",
    "mathjs": "^10.6.4",
    "meteor-babel-helpers": "0.0.3",
    "meteor-node-stubs": "^1.2.24",
    "path": "^0.12.7",
    "tmp": "^0.2.5",
    "underscore": "^1.13.7",
    "y18n": "^5.0.8"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theQRL/block-explorer.git"
  },
  "keywords": [
    "QRL",
    "blockchain"
  ],
  "author": "QRL contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theQRL/block-explorer/issues"
  },
  "homepage": "https://github.com/theQRL/block-explorer#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.6.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-meteor": "^7.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  }
}
