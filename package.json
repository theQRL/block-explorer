  "homepage": "https://github.com/theQRL/block-explorer#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.6.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-meteor": "^7.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  },
  "postcss": {
    "plugins": {
